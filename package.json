{
  "name": "hooks",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.1.5",
  "description": "",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Loogeek/hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Loogeek/hooks.git"
  },
  "bugs": "https://github.com/Loogeek/hooks/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "ts-types": "tsc --emitDeclarationOnly --outDir dist",
    "build": "rm -rf dist && node ./esbuild.js",
    "lint": "eslint .",
    "prepublishOnly": "nr esbuild",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.24.2",
    "@antfu/ni": "^0.16.2",
    "@antfu/utils": "^0.5.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.12",
    "bumpp": "^7.1.1",
    "dom": "link:@testing-library/react-hooks/dom",
    "esbuild": "^0.14.43",
    "eslint": "^8.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.16.3",
    "jsdom": "^20.0.0",
    "pnpm": "^7.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.9"
  },
  "dependencies": {
    "esbuild-plugin-d.ts": "^1.1.0",
    "react": "^18.2.0"
  }
}
